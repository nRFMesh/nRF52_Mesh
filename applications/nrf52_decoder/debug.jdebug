
/*********************************************************************
*                                                                     
*       OnProjectLoad                                                 
*                                                                     
* Function description                                                
*   Project load routine. Required.                                   
*                                                                     
**********************************************************************
*/                                                                    
void OnProjectLoad (void) {
  //
  // Dialog-generated settings
  //
  Project.SetDevice ("nRF52832_xxAA");
  Project.SetHostIF ("USB", "");
  Project.SetTargetIF ("SWD");
  Project.SetTIFSpeed ("4 MHz");
  Project.AddSvdFile ("Cortex-M4F.svd");
  Project.AddSvdFile ("C:/Program Files/SEGGER/Ozone V2.50b/Config/Peripherals/nrf52.svd");
  //
  // User settings
  //
  File.Open ("D:/Dev/nRF52_Mesh/applications/nrf52_decoder/_build/nrf52832_xxaa.out");
}

